
@model  aspmvc.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>



<p>Name: @Model.Name</p>
<ul>
    <li>Subscription : @Model.MembershipType.Name</li>
    
